local ArrayField = loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/Refinement/main/MC%3AArrayfield%20Library"))()
--Documentation url: https://docs.sirius.menu/community/arrayfield

local Window = ArrayField:CreateWindow({
        Name = "Rinns Hub",
        LoadingTitle = "Rinns Hub",
        LoadingSubtitle = "Code by Rinn Ui by Sirius Ui remake by Hosvile",
        ConfigurationSaving = {
            Enabled = true,
            FolderName = RinnsHubConfigs, -- Create a custom folder for your hub/game
            FileName = "RinnsHub"
        },
        Discord = {
            Enabled = true,
            Invite = "Mt9F5nc8Tg", -- The Discord invite code, do not include discord.gg/
            RememberJoins = false -- Set this to false to make them join the discord every time they load it up
        },
        KeySystem = false, -- Set this to true to use our key system
        KeySettings = {
            Title = "ArrayField",
            Subtitle = "Key System",
            Note = "Join the discord (discord.gg/sirius)",
            FileName = "ArrayFieldsKeys",
            SaveKey = false,
            GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like ArrayField to get the key from
            Key = {"Hello",'Bye'},
            Actions = {
                [1] = {
                    Text = 'Click here to copy the key link',
                    OnPress = function()

                    end,
                }
            },
        }
    })
    local Tab = Window:CreateTab("Tab Example", 4483362458) -- Title, Image

local levelAmount = 1

local Label = Tab:CreateLabel("dont skip your units levels or they will break",Section)

local Label = Tab:CreateLabel("go from the units current level then the next level until they are max level",Section)

Tab:CreateInput({
    Name = "Unit Level Amount",
    PlaceholderText = "The Level Your Units Should Be default is 1",
    RemoveTextAfterFocusLost = false,
    Callback = function(Text)
        levelAmount = tonumber(Text)
    end,
})

local function changeUnitLevels(levelAmount)
    for _, child in pairs(workspace.Game.Map.PlacedUnits:GetChildren()) do
        if child:GetAttribute("Level") then
            child:SetAttribute("Level", levelAmount)
        end
    end
end

Tab:CreateButton({
    Name = "Change Units Levels",
    Info = {
        Title = 'This is a Button',
        Description = 'This is a description for the button you know.',
    },
    Interact = 'Changable',
    Callback = function()
        print('Button Pressed')
        changeUnitLevels(levelAmount)
    end,
})

local autoUpgrade = false Tab:CreateToggle({ Name = "Auto Upgrade Units", Info = { Title = 'Auto Upgrade Units', Image = '12735851647', Description = 'Automatically upgrades your units for you', }, CurrentValue = false, Flag = "Toggle1", Callback = function(Value) autoUpgrade = Value if autoUpgrade then while autoUpgrade do local placedUnits = workspace.Game.Map.PlacedUnits:GetChildren() for _, unit in ipairs(placedUnits) do local args = { [1] = unit } game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.7.0") .knit.Services.UnitService.RF.UpgradeUnit:InvokeServer(unpack(args)) end wait(3) end end end, })


